<body>
    <center><h1>These are the Active Forms</h1></center>
    
    
    <table class="table" id="table-container">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Title</th>
                    <th scope="col">Answer</th>
                    
                </tr>
            </thead>
            <tbody>
                <% for(let count=0; count < survey.length; count++) { %>
                    <tr>
                        <th scope="row"><%= count + 1 %></th>

                        <td><%= survey[count].surveyName %></td>

                        
                        <td class="text-center"><button>Answer this survey!</button></td>
                        

     </div>

    <% }) %>

    </div>
</div>
            </tbody>
        </table>








    <!--JavaScript Section-->
